# AWS Configuration
aws_region = "us-east-1"
environment = "dev"

# Lambda Configuration
lambda_timeout = 300
lambda_log_retention_days = 7

# Experiment Configuration
event_bytes = 1024
object_mb = 100

# Factorial Design Factors
lambda_memory_levels = [512, 1024, 2048]
batch_events_levels = [1, 10, 100]
multipart_mb_levels = [8, 32, 64]
reserved_concurrency_levels = [0, 10, 50]

# Project Configuration
project_name = "lambda-s3-experiment"

# S3 Configuration
enable_s3_versioning = true
enable_s3_encryption = true

# Additional Tags
tags = {
  Owner = "devops-team"
  CostCenter = "research"
  Project = "performance-testing"
}
